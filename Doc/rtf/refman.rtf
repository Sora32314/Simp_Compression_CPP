200 \par
201         {\cf19 if}(isRelativeArg)\par
202         \{\par
203             fsArgPath = fs::current_path() / fsArgPath;\par
204         \}\par
205 \par
206         {\cf20 //\'B4\'F2\'BF\'AA\'CE\'C4\'BC\'FE}\par
207         std::ifstream argFile(fsArgPath, std::ios::in);\par
208         std::ofstream resFile(fsResPath, std::ios::out|std::ios::trunc|std::ios::binary);\par
209 \par
210         {\cf20 //\'CE\'C4\'BC\'FE\'B3\'A4\'B6\'C8}\par
211         {\cf18 size_t} length = 0;\par
212         resFile.write({\cf17 reinterpret_cast<}{\cf18 char}*{\cf17 >}(&length), {\cf17 sizeof}({\cf18 size_t}));\par
213 \par
214         {\cf20 //\'D6\'F0\'BD\'A5\'B6\'C1\'C8\'A1\'CE\'C4\'BC\'FE\'D6\'B1\'B5\'BD\'CE\'C4\'BC\'FE\'C4\'A9\'CE\'B2}\par
215         {\cf18 char} buffer[128] = \{0\};\par
216         {\cf19 while} (!argFile.eof())\par
217         \{\par
218             {\cf20 //\'B4\'B4\'BD\'A8\'BB\'BA\'B3\'E5\'C7\'F8\'B2\'A2\'B3\'F5\'CA\'BC\'BB\'AF\'D7\'D6\'B7\'FB\'B4\'AE}\par
219             argFile.read(buffer, 128);\par
220             std::streamsize bytesRead = argFile.gcount();\par
221             \par
222             {\cf20 //\'BD\'AB\'B6\'C1\'C8\'A1\'B5\'BD\'B5\'C4\'CA\'FD\'BE\'DD\'B4\'E6\'B4\'A2\'D6\'C1\'CA\'FD\'D7\'E9\'D6\'D0}\par
223             {\cf19 for}({\cf17 decltype}(bytesRead) i = 0; i < bytesRead; i++, length += 2)\par
224             \{\par
225                 {\cf19 if}(buffer[i] == {\cf23 'A'})\par
226                 \{\par
227                     streamTarget.push_back(0);\par
228                     streamTarget.push_back(0);\par
229                 \}\par
230                 {\cf19 else} {\cf19 if}(buffer[i] == {\cf23 'T'})\par
231                 \{\par
232                     streamTarget.push_back(1);\par
233                     streamTarget.push_back(0);\par
234                 \}\par
235                 {\cf19 else} {\cf19 if}(buffer[i] == {\cf23 'C'})\par
236                 \{\par
237                     streamTarget.push_back(0);\par
238                     streamTarget.push_back(1);\par
239                 \}\par
240                 {\cf19 else} {\cf19 if}(buffer[i] == {\cf23 'G'})\par
241                 \{\par
242                     streamTarget.push_back(1);\par
243                     streamTarget.push_back(1);\par
244                 \}\par
245             \}\par
246 \par
247             {\cf20 //\'B4\'A6\'C0\'ED\'B2\'A2\'D0\'B4\'C8\'EB\'CA\'FD\'BE\'DD\'B5\'BDint\'D6\'D0\'A3\'AC\'D6\'AE\'BA\'F3\'D0\'B4\'C8\'EB\'B5\'BD\'CE\'C4\'BC\'FE\'C0\'EF}\par
248             {\cf19 for} ({\cf18 size_t} i = 0; i < streamTarget.size();) \par
249             \{\par
250                 target = 0;\par
251                 {\cf19 for} ({\cf18 int} j = 1; j <= intBitCount && i < streamTarget.size(); j++, i++) \par
252                 \{\par
253                     target = BO::Bit_Set(target, j, streamTarget[i]);\par
254                 \}\par
255                 resFile.write({\cf17 reinterpret_cast<}{\cf18 char}*{\cf17 >}(&target), {\cf17 sizeof}({\cf18 int}));\par
256             \}\par
257             \par
258             streamTarget.clear();\par
259         \}   \par
260 \par
261         resFile.seekp(std::ios::beg);\par
262         resFile.write({\cf17 reinterpret_cast<}{\cf18 char}*{\cf17 >}(&length), {\cf17 sizeof}({\cf18 size_t}));\par
263 \par
264         argFile.close();\par
265         resFile.close();\par
266         \par
267         {\cf19 return} {\cf17 true};\par
268     \}\par
