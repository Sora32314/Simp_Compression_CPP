281     {\cf18 bool} Gene_Decompression(std::string&& argPath, {\cf18 bool} isRelativeArg, std::string&& resPath, {\cf18 bool} isRelativeRes)\par
282     \{\par
283         std::vector<bool> streamTarget;\par
284         std::vector<char> streamInput;\par
285         fs::path fsResPath(std::forward<std::string>(resPath));\par
286         fs::path fsArgPath(std::forward<std::string>(argPath));\par
287         {\cf20 //\'C8\'E7\'B9\'FB\'CA\'C7\'CF\'E0\'B6\'D4\'C2\'B7\'BE\'B6\'A3\'AC\'D4\'F2\'B2\'B9\'B3\'E4\'CE\'AA\'BE\'F8\'B6\'D4\'C2\'B7\'BE\'B6}\par
288         {\cf19 if}(isRelativeRes)\par
289         \{\par
290             fsResPath = fs::current_path() / fsResPath;\par
291         \}\par
292 \par
293         {\cf19 if}(isRelativeArg)\par
294         \{\par
295             fsArgPath = fs::current_path() / fsArgPath;\par
296         \}\par
297 \par
298         std::ifstream ArgFile(fsArgPath, std::ios::in|std::ios::binary);\par
299         std::ofstream ResFile(fsResPath, std::ios::ate|std::ios::trunc);\par
300 \par
301 \par
302         {\cf20 //\'BB\'F1\'C8\'A1\'CE\'C4\'BC\'FE\'B3\'A4\'B6\'C8}\par
303         {\cf18 char} lengthBuffer[{\cf17 sizeof}(size_t)] = \{0\};\par
304         ArgFile.read(lengthBuffer, {\cf17 sizeof}({\cf18 size_t}));\par
305         {\cf18 size_t} size = *({\cf17 reinterpret_cast<}{\cf18 size_t}*{\cf17 >}(lengthBuffer));\par
306         \par
307         \par
308         {\cf19 while}(size > 0)\par
309         \{\par
310             {\cf20 //\'B4\'B4\'BD\'A8\'B2\'A2\'B3\'F5\'CA\'BC\'BB\'AF\'D7\'D6\'B7\'FB\'B4\'AE}\par
311             {\cf18 char} buffer[128] = \{0\};\par
312             ArgFile.read(buffer, 128);\par
313             std::streamsize bytesRead = ArgFile.gcount();\par
314 \par
315             {\cf20 //\'B6\'C1\'C8\'A1\'C3\'BF\'D2\'BB\'CE\'BB\'B5\'BDbool\'C0\'E0\'D0\'CD\'CA\'FD\'D7\'E9\'D6\'D0}\par
316             {\cf19 for}({\cf17 decltype}(bytesRead) i = 0; i < bytesRead; i++)\par
317             \{\par
318                 {\cf19 for}({\cf17 decltype}(bytesRead) j = 0; j < BYTESIZE; j++)\par
319                 \{\par
320                     streamTarget.push_back(BO::Bit_Check(buffer[i], j));\par
321                 \}\par
322             \}\par
323             \par
324 \par
325 \par
326             {\cf19 for}({\cf18 size_t} bufferCount = 0; bufferCount < streamTarget.size() && size > 0; bufferCount += 2, size -= 2)\par
327             \{\par
328                 {\cf19 if}(streamTarget[bufferCount] == {\cf17 false})\par
329                 \{\par
330                     {\cf19 if}(streamTarget[bufferCount + 1] == {\cf17 false})\par
331                     \{\par
332                         streamInput.push_back({\cf23 'A'});\par
333                     \}\par
334                     {\cf19 else}\par
335                     \{\par
336                         streamInput.push_back({\cf23 'C'});\par
337                     \}\par
338                 \}\par
339                 {\cf19 else} {\cf19 if}(streamTarget[bufferCount] == {\cf17 true})\par
340                 \{\par
341                     {\cf19 if}(streamTarget[bufferCount + 1] == {\cf17 false})\par
342                     \{\par
343                         streamInput.push_back({\cf23 'T'});\par
344                     \}\par
345                     {\cf19 else}\par
346                     \{\par
347                         streamInput.push_back({\cf23 'G'});\par
348                     \}\par
349                 \}\par
350             \}\par
351 \par
352             streamTarget.clear();\par
353             \par
354             ResFile.write(streamInput.data(), streamInput.size());\par
355             \par
356             streamInput.clear();\par
357         \}\par
358         \par
359         ArgFile.close();\par
360         ResFile.close();\par
361 \par
362         {\cf19 return} {\cf17 true};\par
363     \}\par
